{
  "name": "express-starter-ts",
  "version": "1.0.0",
  "description": "This repository gives the developer an Express Starter written in Typescript to kick off there development fast ðŸ”¥ðŸ”¥ðŸ”¥",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha ./dist/tests/*.test.js ./dist/tests/**/*.controller.test.js",
    "pretest": "tslint src/**/**/*.*.ts rimraf ./dist && npm run build",
    "start": "npm run build && node ./dist/server.js",
    "build": "rimraf ./dist && tsc --outDir ./dist",
    "watch": "nodemon",
    "setup:config": "gulp createTravisOrmConfig",
    "dbmigrate:up": "npm run build && typeorm migrations:run",
    "dbmigrate:down": "npm run build && typeorm migrations:revert",
    "dbmigrate:create": "typeorm migrations:create -n",
    "sh": "npm run setup:config && bash src/scripts/before.travis.script.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmaicaaan/express-starter-ts.git"
  },
  "author": "JM Santos",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jmaicaaan/express-starter-ts/issues"
  },
  "homepage": "https://github.com/jmaicaaan/express-starter-ts#readme",
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.2",
    "lodash": "^4.17.4",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.5",
    "pg": "^7.4.0",
    "reflect-metadata": "^0.1.10",
    "routing-controllers": "^0.7.6",
    "typedi": "^0.6.0",
    "typeorm": "^0.1.9",
    "typeorm-typedi-extensions": "^0.1.1",
    "typescript": "^2.6.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^1.0.0",
    "@types/body-parser": "^1.16.8",
    "@types/chai": "^4.1.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.0.39",
    "@types/lodash": "^4.14.92",
    "@types/mocha": "^2.2.46",
    "@types/node": "^8.0.56",
    "chai": "^4.1.2",
    "cross-env": "^5.1.3",
    "gulp": "^3.9.1",
    "gulp-delete-lines": "0.0.7",
    "gulp-insert": "^0.5.0",
    "gulp-rename": "^1.2.2",
    "gulp-template": "^4.0.0",
    "rimraf": "^2.6.2",
    "supertest": "^3.0.0",
    "tslint": "^5.9.1",
    "yargs": "^10.0.3"
  }
}
