{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "no-console": [
      false
    ],
    "no-shadowed-variable": false,
    "no-var-requires": false,
    "object-literal-sort-keys": false,
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "member-access": true,
    "member-ordering": false,
    "no-implicit-dependencies": false,
    "object-literal-shorthand": false
  },
  "rulesDirectory": []
}
